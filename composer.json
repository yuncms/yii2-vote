{
  "name": "yuncms/yii2-vote",
  "description": "Provides anonymous voting for any models",
  "keywords": [
    "vote",
    "anonymous",
    "voting"
  ],
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/yuncms/yii2-vote/issues",
    "source": "https://github.com/yuncms/yii2-vote"
  },
  "authors": [
    {
      "name": "Vladimir Babin",
      "email": "vovababin@gmail.com",
      "homepage": "http://chiliec.ru"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "yuncms\\vote\\": ""
    }
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "^2.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  ]
}
